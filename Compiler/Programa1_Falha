Falta = 10;
Nota_1 = 10;
Nota_2= 4;
Nota_3 = 8;
//Erro de falha semantica de numero maior que 9999
situacao = 100000000000;
Media = (Nota_1+Nota_2+Nota_3)/3;
//0 Aprovado 1 Reprovado

loop(Nota_1 >= Nota_2){

    casofor((Falta>10) || (Media<7)){
        situacao = 1;
        imprimir situacao;
    }naofoi {
        imprimir situacao;
    }

}

